<div class="container my_search">
  <div id="flightSearchBox">
    <div style="float:left">
      <div class="styled-select blue" style="margin:4px;height:50px">
        <select>
          <option>Round Trip</option>
          <option>One Way</option>
          <option>Multi Cities</option>
        </select>
      </div>
      <div class="input-group" style="margin:8px 4px; border-radius:0px !important; width:300px">
        <input type="text" id="txtflightDepartLocation" placeholder="From" class="form-control" style="border-radius:0px !important;height:50px;  width:311px;"/>
        <span class="input-group-addon" style="border-radius:0px !important;"><img src="images/map.png" ></span> </div>
      <div class="input-group" style="margin:8px 4px; border-radius:0px !important; width:300px">
        <input type="text" id="txtflightArrivalLocation" placeholder="To" class="form-control" style="border-radius:0px !important;height:50px; width:311px;"/>
        <span class="input-group-addon" style="border-radius:0px !important;"><img src="images/map.png" ></span> </div>
    </div>
    <div style="float:left">
      <div class="departureDate">
        <div class="sub_title"> Departure Date </div>
        <div class="sub_departureDate" >
          <div class="day"> THURSDAY </div>
          <div class="date"> 24 </div>
          <div class="month"> Dec </div>
          <div class="year" > 2014 </div>
        </div>
      </div>
    </div>
    <div style="float:left">
      <div class="departureDate">
        <div class="sub_title"> Arrival Date </div>
        <div class="sub_departureDate" >
          <div class="day"> THURSDAY </div>
          <div class="date"> 24 </div>
          <div class="month"> Dec </div>
          <div class="year" > 2014 </div>
        </div>
      </div>
    </div>
    <div class="passengerCount" style="margin:8px 4px; border-radius:0px !important; float: left;letter-spacing: 0;border: 6px solid #1D3699;margin: 6px 2px 8px;">
      <div class="passengerCount_subtitle">PASSANGERS</div>
      <div style="margin-top: -38px;margin-left: 30px;">
        <div class="adultCount passengerSubCount" style="display:block"> <span class="title" style="display:inline-block">ADULT</span> <span class="count" style="display:inline-block">1</span> </div>
        <div class="seniorCount passengerSubCount" style="display:block"> <span class="title" style="display:inline-block">SENIOR</span> <span class="count" style="display:inline-block">0</span> </div>
        <div class="childrenCount passengerSubCount" style="display:block"> <span class="title" style="display:inline-block">CHILDREN</span> <span class="count" style="display:inline-block">0</span> </div>
        <div class="infantLapCount passengerSubCount" style="display:block"> <span class="title" style="display:inline-block">INFANT(lap)</span> <span class="count" style="display:inline-block">0</span> </div>
        <div class="infantSeatCount passengerSubCount" style="display:block"> <span class="title" style="display:inline-block">INFANT(seat)</span> <span class="count" style="display:inline-block">0</span> </div>
      </div>
      <div class="passengerCount_total "> <span class="passengerCount_total_title">TOTAL</span> <span class="passengerCount_total_number">1</span> </div>
    </div>
    <div class="moreOptions">MORE OPTIONS</div>
    <div class="divSearchButton"  >
      <input type="button" value="SEARCH" class="btn btn-danger" >
    </div>
  </div>
</div>
